{
    "apiVersion": "v1",
    "kind": "Service",
    "metadata": {
        "vendor": "jmccormick2001",
        "name": "{{.ServiceName}}",
        "namespace": "{{.Namespace}}"
    },
    "spec": {
        "ports": [
            {
                "name": "rq",
                "port": 4001,
                "protocol": "TCP",
                "targetPort": 4001
            }
        ],
        "selector": {
            {{.LeaderStatus}}
            "cluster": "{{.ClusterName}}"
        },
        "sessionAffinity": "None",
        "type": "ClusterIP"
    }
}

